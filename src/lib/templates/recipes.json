etail View */\n\t.detail {\n\t\tmax-width: 700px;\n\t\tmargin: 0 auto;\n\t\tpadding-bottom: 2rem;\n\t}\n\n\t.detail-header {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tjustify-content: space-between;\n\t\tpadding: 0 4px;\n\t}\n\n\t.detail-actions {\n\t\tdisplay: flex;\n\t\tgap: 8px;\n\t}\n\n\t.action-btn {\n\t\tpadding: 8px;\n\t\tbackground: none;\n\t\tborder: 1px solid var(--border-color, #e8e0d8);\n\t\tborder-radius: 8px;\n\t\tcolor: var(--secondary-text-color, #78716c);\n\t\tcursor: pointer;\n\t\ttransition: all 0.15s;\n\t}\n\n\t.action-btn:hover {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tcolor: var(--heading-text-color, #292524);\n\t}\n\n\t.action-btn.delete:hover {\n\t\tcolor: #dc2626;\n\t\tborder-color: #dc2626;\n\t}\n\n\t.back-btn {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 6px;\n\t\tpadding: 12px 16px;\n\t\tbackground: none;\n\t\tborder: none;\n\t\tcolor: var(--secondary-text-color, #78716c);\n\t\tfont-size: 0.9rem;\n\t\tcursor: pointer;\n\t}\n\n\t.back-btn:hover {\n\t\tcolor: var(--heading-text-color, #292524);\n\t}\n\n\t.hero-image {\n\t\taspect-ratio: 16/9;\n\t\toverflow: hidden;\n\t}\n\n\t.hero-image img {\n\t\twidth: 100%;\n\t\theight: 100%;\n\t\tobject-fit: cover;\n\t}\n\n\t.detail-content {\n\t\tpadding: 1.5rem;\n\t}\n\n\t.detail-content h1 {\n\t\tmargin: 0 0 1rem 0;\n\t\tcolor: var(--heading-text-color, #292524);\n\t\tfont-size: 1.75rem;\n\t}\n\n\t.meta {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 16px;\n\t\tmargin-bottom: 1rem;\n\t\tflex-wrap: wrap;\n\t}\n\n\t.meta-item {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 6px;\n\t\tcolor: var(--secondary-text-color, #78716c);\n\t\tfont-size: 0.9rem;\n\t}\n\n\t.category-badge {\n\t\tpadding: 4px 12px;\n\t\tbackground: var(--cat-badge-bg, #fef3c7);\n\t\tcolor: var(--accent-color, #d97706);\n\t\tborder-radius: 20px;\n\t\tfont-size: 0.85rem;\n\t}\n\n\t.description {\n\t\tcolor: var(--body-text-color, #44403c);\n\t\tline-height: 1.6;\n\t\tmargin-botto