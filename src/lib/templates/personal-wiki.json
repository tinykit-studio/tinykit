{
	"id": "personal-wiki",
	"name": "Personal Wiki",
	"description": "Zettelkasten-style linked notes",
	"preview": "lucide:brain",
	"tags": [
		"wiki",
		"notes",
		"advanced"
	],
	"kits": [
		"wiki",
		"daily"
