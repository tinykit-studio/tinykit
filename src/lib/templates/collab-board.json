{
	"id": "collab-board",
	"name": "Collab Board",
	"description": "Showcase collaboration opportunities and availability",
	"preview": "lucide:handshake",
	"tags": [
		"creator",
		"intermediate",
		"portfolio"
	],
	"kits": [
		"creator"
	],
	"frontend_code": "<script>\n\timport Icon from '@iconify/svelte'\n\timport content from '$content'\n\timport data from '$data'\n\n\tlet collab_types = $state([])\n\tlet past_collabs = $state([])\n\tlet loading = $state(true)\n\n\t// Form state\n\tlet form_name = $state('')\n\tlet form_email = $state('')\n\tlet form_brand = $state('')\n\tlet form_type = $state('')\n\tlet form_message = $state('')\n\tlet form_submitted = $state(false)\n\tlet form_error = $state('')\n\n\tdata.collab_types.subscribe(items => {\n\t\tcollab_types = items.sort((a, b) => (a.order || 0) - (b.order || 0))\n\t})\n\n\tdata.past_collabs.subscribe(items => {\n\t\tpast_collabs = items.sort((a, b) => (a.order || 0) - (b.order || 0))\n\t\tloading = false\n\t})\n\n\tasync function handle_submit(e) {\n\t\te.preventDefault()\n\t\tform_error = ''\n\n\t\tif (!form_name || !form_email || !form_type || !form_message) {\n\t\t\tform_error = 'Please fill in all required fields'\n\t\t\treturn\n\t\t}\n\n\t\t// Simulate form submission\n\t\tawait new Promise(r => setTimeout(r, 500))\n\t\tform_submitted = true\n\t}\n\n\tfunction reset_form() {\n\t\tform_name = ''\n\t\tform_email = ''\n\t\tform_brand = ''\n\t\tform_type = ''\n\t\tform_message = ''\n\t\tform_submitted = false\n\t\tform_error = ''\n\t}\n</script>\n\n<div class=\"page\">\n\t<header class=\"hero\">\n\t\t<div class=\"hero-content\">\n\t\t\t<div class=\"status-badge\" class:open={content.open_to_collabs === 'true'}>\n\t\t\t\t<span class=\"status-dot\"></span>\n\t\t\t\t{content.open_to_collabs === 'true' ? 'Open to Collaborations' : 'Currently Booked'}\n\t\t\t</div>\n\t\t\t<h1>{content.headline}</h1>\n\t\t\t<p class=\"subheadline\">{content.subheadline}</p>\n\t\t</div>\n\t</header>\n\n\t<main class=\"container\">\n\t\t<section class=\"section intro\">\n\t\t\t<div class=\"intro-content\">\n\t\t\t\t<img src={content.photo_url} alt={content.creator_name} class=\"creator-photo\" />\n\t\t\t\t<div class=\"intro-text\">\n\t\t\t\t\t<h2>Hi, I'm {content.creator_name}!</h2>\n\t\t\t\t\t<p>{content.intro_text}</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</section>\n\n\t\t<section class=\"section\">\n\t\t\t<h2 class=\"section-title\">\n\t\t\t\t<Icon icon=\"lucide:sparkles\" width={24} />\n\t\t\t\tTypes of Collabs I'm Interested In\n\t\t\t</h2>\n\t\t\t<div class=\"collab-types-grid\">\n\t\t\t\t{#each collab_types as type (type.id)}\n\t\t\t\t\t<div class=\"collab-type-card\">\n\t\t\t\t\t\t<div class=\"type-icon\">\n\t\t\t\t\t\t\t<Icon icon={type.icon || 'lucide:star'} width={28} />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<h3>{type.name}</h3>\n\t\t\t\t\t\t<p>{type.description}</p>\n\t\t\t\t\t\t{#if type.rate}\n\t\t\t\t\t\t\t<span class=\"type-rate\">{type.rate}</span>\n\t\t\t\t\t\t{/if}\n\t\t\t\t\t</div>\n\t\t\t\t{/each}\n\t\t\t</div>\n\t\t</section>\n\n\t\t<section class=\"section requirements\">\n\t\t\t<h2 class=\"section-title\">\n\t\t\t\t<Icon icon=\"lucide:clipboard-check\" width={24} />\n\t\t\t\tRequirements & Guidelines\n\t\t\t</h2>\n\t\t\t<div class=\"requirements-grid\">\n\t\t\t\t<div class=\"requirement-card\">\n\t\t\t\t\t<Icon icon=\"lucide:check-circle\" width={20} />\n\t\t\t\t\t<p>{content.requirement_1}</p>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"requirement-card\">\n\t\t\t\t\t<Icon icon=\"lucide:check-circle\" width={20} />\n\t\t\t\t\t<p>{content.requirement_2}</p>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"requirement-card\">\n\t\t\t\t\t<Icon icon=\"lucide:check-circle\" width={20} />\n\t\t\t\t\t<p>{content.requirement_3}</p>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"requirement-card\">\n\t\t\t\t\t<Icon icon=\"lucide:check-circle\" width={20} />\n\t\t\t\t\t<p>{content.requirement_4}</p>\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</section>\n\n\t\t{#if content.show_calendar === 'true' && content.calendar_embed}\n\t\t\t<section class=\"section\">\n\t\t\t\t<h2 class=\"section-title\">\n\t\t\t\t\t<Icon icon=\"lucide:calendar\" width={24} />\n\t\t\t\t\tMy Availability\n\t\t\t\t</h2>\n\t\t\t\t<div class=\"calendar-container\">\n\t\t\t\t\t<p class=\"calendar-note\">{content.availability_note}</p>\n\t\t\t\t\t{#if content.calendar_url}\n\t\t\t\t\t\t<a href={content.calendar_url} target=\"_blank\" rel=\"noopener\" class=\"calendar-btn\">\n\t\t\t\t\t\t\t<Icon icon=\"lucide:calendar-plus\" width={20} />\n\t\t\t\t\t\t\tBook a Discovery Call\n\t\t\t\t\t\t</a>\n\t\t\t\t\t{/if}\n\t\t\t\t</div>\n\t\t\t</section>\n\t\t{/if}\n\n\t\t<section class=\"section\">\n\t\t\t<h2 class=\"section-title\">\n\t\t\t\t<Icon icon=\"lucide:trophy\" width={24} />\n\t\t\t\tPast Collaborations\n\t\t\t</h2>\n\t\t\t<div class=\"past-collabs-grid\">\n\t\t\t\t{#each past_collabs as collab (collab.id)}\n\t\t\t\t\t<div class=\"past-collab-card\">\n\t\t\t\t\t\t{#if collab.image_url}\n\t\t\t\t\t\t\t<img src={collab.image_url} alt={collab.brand} class=\"collab-image\" />\n\t\t\t\t\t\t{/if}\n\t\t\t\t\t\t<div class=\"collab-info\">\n\t\t\t\t\t\t\t<span class=\"collab-brand\">{collab.brand}</span>\n\t\t\t\t\t\t\t<span class=\"collab-type\">{collab.type}</span>\n\t\t\t\t\t\t\t{#if collab.description}\n\t\t\t\t\t\t\t\t<p>{collab.description}</p>\n\t\t\t\t\t\t\t{/if}\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\t\t\t\t{/each}\n\t\t\t</div>\n\t\t</section>\n\n\t\t<section class=\"section inquiry-section\">\n\t\t\t<h2 class=\"section-title\">\n\t\t\t\t<Icon icon=\"lucide:send\" width={24} />\n\t\t\t\tSend an Inquiry\n\t\t\t</h2>\n\n\t\t\t{#if form_submitted}\n\t\t\t\t<div class=\"success-message\">\n\t\t\t\t\t<Icon icon=\"lucide:check-circle\" width={48} />\n\t\t\t\t\t<h3>Thanks for reaching out!</h3>\n\t\t\t\t\t<p>I'll get back to you within {content.response_time}.</p>\n\t\t\t\t\t<button class=\"reset-btn\" onclick={reset_form}>Submit Another</button>\n\t\t\t\t</div>\n\t\t\t{:else}\n\t\t\t\t<form class=\"inquiry-form\" onsubmit={handle_submit}>\n\t\t\t\t\t{#if form_error}\n\t\t\t\t\t\t<div class=\"form-error\">\n\t\t\t\t\t\t\t<Icon icon=\"lucide:alert-circle\" width={16} />\n\t\t\t\t\t\t\t{form_error}\n\t\t\t\t\t\t</div>\n\t\t\t\t\t{/if}\n\n\t\t\t\t\t<div class=\"form-row\">\n\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t<label for=\"name\">Your Name *</label>\n\t\t\t\t\t\t\t<input type=\"text\" id=\"name\" bind:value={form_name} placeholder=\"Jane Smith\" />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t<label for=\"email\">Email *</label>\n\t\t\t\t\t\t\t<input type=\"email\" id=\"email\" bind:value={form_email} placeholder=\"jane@brand.com\" />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div class=\"form-row\">\n\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t<label for=\"brand\">Brand/Company</label>\n\t\t\t\t\t\t\t<input type=\"text\" id=\"brand\" bind:value={form_brand} placeholder=\"Awesome Brand Co.\" />\n\t\t\t\t\t\t</div>\n\t\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t\t<label for=\"type\">Collaboration Type *</label>\n\t\t\t\t\t\t\t<select id=\"type\" bind:value={form_type}>\n\t\t\t\t\t\t\t\t<option value=\"\">Select type...</option>\n\t\t\t\t\t\t\t\t{#each collab_types as type (type.id)}\n\t\t\t\t\t\t\t\t\t<option value={type.name}>{type.name}</option>\n\t\t\t\t\t\t\t\t{/each}\n\t\t\t\t\t\t\t\t<option value=\"Other\">Other</option>\n\t\t\t\t\t\t\t</select>\n\t\t\t\t\t\t</div>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<div class=\"form-group\">\n\t\t\t\t\t\t<label for=\"message\">Tell me about your project *</label>\n\t\t\t\t\t\t<textarea id=\"message\" bind:value={form_message} rows=\"4\" placeholder=\"What are you looking to create together?\"></textarea>\n\t\t\t\t\t</div>\n\n\t\t\t\t\t<button type=\"submit\" class=\"submit-btn\">\n\t\t\t\t\t\t<Icon icon=\"lucide:send\" width={18} />\n\t\t\t\t\t\tSend Inquiry\n\t\t\t\t\t</button>\n\t\t\t\t</form>\n\t\t\t{/if}\n\t\t</section>\n\t</main>\n\n\t<footer class=\"footer\">built with <a href=\"https://tinykit.studio\" target=\"_blank\">tinykit.studio</a></footer>\n</div>\n\n<style>\n\t.page {\n\t\tmin-height: 100vh;\n\t\tbackground: var(--page-background, #fafafa);\n\t\tfont-family: var(--body-font, 'Inter', system-ui, sans-serif);\n\t\tcolor: var(--body-text-color, #1a1a1a);\n\t}\n\n\t.hero {\n\t\tbackground: linear-gradient(135deg, var(--gradient-start, #a855f7) 0%, var(--gradient-end, #ec4899) 100%);\n\t\tpadding: 5rem 2rem;\n\t\ttext-align: center;\n\t}\n\n\t.hero-content {\n\t\tmax-width: 700px;\n\t\tmargin: 0 auto;\n\t}\n\n\t.status-badge {\n\t\tdisplay: inline-flex;\n\t\talign-items: center;\n\t\tgap: 0.5rem;\n\t\tbackground: rgba(255, 255, 255, 0.2);\n\t\tpadding: 0.5rem 1rem;\n\t\tborder-radius: 50px;\n\t\tfont-size: 0.85rem;\n\t\tfont-weight: 600;\n\t\tcolor: #ffffff;\n\t\tmargin-bottom: 1.5rem;\n\t}\n\n\t.status-dot {\n\t\twidth: 10px;\n\t\theight: 10px;\n\t\tborder-radius: 50%;\n\t\tbackground: #ef4444;\n\t}\n\n\t.status-badge.open .status-dot {\n\t\tbackground: #22c55e;\n\t\tanimation: pulse 2s infinite;\n\t}\n\n\t@keyframes pulse {\n\t\t0%, 100% { opacity: 1; }\n\t\t50% { opacity: 0.5; }\n\t}\n\n\t.hero h1 {\n\t\tmargin: 0;\n\t\tfont-size: 2.75rem;\n\t\tfont-weight: 800;\n\t\tcolor: #ffffff;\n\t\tline-height: 1.2;\n\t}\n\n\t.subheadline {\n\t\tmargin: 1rem 0 0;\n\t\tfont-size: 1.15rem;\n\t\tcolor: rgba(255, 255, 255, 0.9);\n\t}\n\n\t.container {\n\t\tmax-width: 900px;\n\t\tmargin: 0 auto;\n\t\tpadding: 2rem;\n\t}\n\n\t.section {\n\t\tmargin-bottom: 4rem;\n\t}\n\n\t.section-title {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 0.75rem;\n\t\tfont-size: 1.5rem;\n\t\tfont-weight: 700;\n\t\tcolor: var(--heading-color, #1a1a1a);\n\t\tmargin: 0 0 1.5rem 0;\n\t}\n\n\t.section-title :global(svg) {\n\t\tcolor: var(--accent-color, #a855f7);\n\t}\n\n\t.intro {\n\t\tmargin-top: -3rem;\n\t}\n\n\t.intro-content {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tborder-radius: var(--card-radius, 20px);\n\t\tpadding: 2rem;\n\t\tbox-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n\t\tdisplay: flex;\n\t\tgap: 2rem;\n\t\talign-items: center;\n\t\tflex-wrap: wrap;\n\t}\n\n\t.creator-photo {\n\t\twidth: 120px;\n\t\theight: 120px;\n\t\tborder-radius: 50%;\n\t\tobject-fit: cover;\n\t\tborder: 4px solid var(--accent-color, #a855f7);\n\t}\n\n\t.intro-text {\n\t\tflex: 1;\n\t\tmin-width: 250px;\n\t}\n\n\t.intro-text h2 {\n\t\tmargin: 0 0 0.75rem 0;\n\t\tfont-size: 1.5rem;\n\t\tcolor: var(--heading-color, #1a1a1a);\n\t}\n\n\t.intro-text p {\n\t\tmargin: 0;\n\t\tcolor: var(--secondary-text-color, #666666);\n\t\tline-height: 1.7;\n\t}\n\n\t.collab-types-grid {\n\t\tdisplay: grid;\n\t\tgrid-template-columns: repeat(auto-fit, minmax(220px, 1fr));\n\t\tgap: 1.25rem;\n\t}\n\n\t.collab-type-card {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tborder-radius: var(--card-radius, 16px);\n\t\tpadding: 1.5rem;\n\t\tbox-shadow: 0 2px 12px rgba(0, 0, 0, 0.06);\n\t\ttransition: all 0.2s;\n\t}\n\n\t.collab-type-card:hover {\n\t\ttransform: translateY(-4px);\n\t\tbox-shadow: 0 8px 24px rgba(0, 0, 0, 0.1);\n\t}\n\n\t.type-icon {\n\t\twidth: 56px;\n\t\theight: 56px;\n\t\tbackground: linear-gradient(135deg, var(--gradient-start, #a855f7), var(--gradient-end, #ec4899));\n\t\tborder-radius: 12px;\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tjustify-content: center;\n\t\tcolor: #ffffff;\n\t\tmargin-bottom: 1rem;\n\t}\n\n\t.collab-type-card h3 {\n\t\tmargin: 0 0 0.5rem 0;\n\t\tfont-size: 1.1rem;\n\t\tcolor: var(--heading-color, #1a1a1a);\n\t}\n\n\t.collab-type-card p {\n\t\tmargin: 0 0 0.75rem 0;\n\t\tcolor: var(--secondary-text-color, #666666);\n\t\tfont-size: 0.9rem;\n\t\tline-height: 1.5;\n\t}\n\n\t.type-rate {\n\t\tdisplay: inline-block;\n\t\tbackground: rgba(168, 85, 247, 0.1);\n\t\tcolor: var(--accent-color, #a855f7);\n\t\tpadding: 0.25rem 0.75rem;\n\t\tborder-radius: 20px;\n\t\tfont-size: 0.8rem;\n\t\tfont-weight: 600;\n\t}\n\n\t.requirements-grid {\n\t\tdisplay: grid;\n\t\tgrid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n\t\tgap: 1rem;\n\t}\n\n\t.requirement-card {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tborder-radius: var(--card-radius, 12px);\n\t\tpadding: 1.25rem;\n\t\tdisplay: flex;\n\t\talign-items: flex-start;\n\t\tgap: 0.75rem;\n\t\tbox-shadow: 0 2px 8px rgba(0, 0, 0, 0.04);\n\t}\n\n\t.requirement-card :global(svg) {\n\t\tcolor: var(--success-color, #22c55e);\n\t\tflex-shrink: 0;\n\t\tmargin-top: 2px;\n\t}\n\n\t.requirement-card p {\n\t\tmargin: 0;\n\t\tcolor: var(--body-text-color, #1a1a1a);\n\t\tfont-size: 0.95rem;\n\t\tline-height: 1.5;\n\t}\n\n\t.calendar-container {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tborder-radius: var(--card-radius, 16px);\n\t\tpadding: 2rem;\n\t\ttext-align: center;\n\t\tbox-shadow: 0 2px 12px rgba(0, 0, 0, 0.06);\n\t}\n\n\t.calendar-note {\n\t\tmargin: 0 0 1.5rem 0;\n\t\tcolor: var(--secondary-text-color, #666666);\n\t}\n\n\t.calendar-btn {\n\t\tdisplay: inline-flex;\n\t\talign-items: center;\n\t\tgap: 0.5rem;\n\t\tbackground: linear-gradient(90deg, var(--gradient-start, #a855f7), var(--gradient-end, #ec4899));\n\t\tcolor: #ffffff;\n\t\tpadding: 0.875rem 1.5rem;\n\t\tborder-radius: 50px;\n\t\tfont-weight: 600;\n\t\ttext-decoration: none;\n\t\ttransition: all 0.2s;\n\t}\n\n\t.calendar-btn:hover {\n\t\ttransform: translateY(-2px);\n\t\tbox-shadow: 0 4px 20px rgba(168, 85, 247, 0.4);\n\t}\n\n\t.past-collabs-grid {\n\t\tdisplay: grid;\n\t\tgrid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n\t\tgap: 1.25rem;\n\t}\n\n\t.past-collab-card {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tborder-radius: var(--card-radius, 16px);\n\t\toverflow: hidden;\n\t\tbox-shadow: 0 2px 12px rgba(0, 0, 0, 0.06);\n\t}\n\n\t.collab-image {\n\t\twidth: 100%;\n\t\theight: 150px;\n\t\tobject-fit: cover;\n\t}\n\n\t.collab-info {\n\t\tpadding: 1.25rem;\n\t}\n\n\t.collab-brand {\n\t\tdisplay: block;\n\t\tfont-size: 1.1rem;\n\t\tfont-weight: 700;\n\t\tcolor: var(--heading-color, #1a1a1a);\n\t\tmargin-bottom: 0.25rem;\n\t}\n\n\t.collab-type {\n\t\tdisplay: inline-block;\n\t\tbackground: rgba(168, 85, 247, 0.1);\n\t\tcolor: var(--accent-color, #a855f7);\n\t\tpadding: 0.2rem 0.6rem;\n\t\tborder-radius: 4px;\n\t\tfont-size: 0.75rem;\n\t\tfont-weight: 600;\n\t\ttext-transform: uppercase;\n\t\tmargin-bottom: 0.75rem;\n\t}\n\n\t.collab-info p {\n\t\tmargin: 0;\n\t\tcolor: var(--secondary-text-color, #666666);\n\t\tfont-size: 0.9rem;\n\t\tline-height: 1.5;\n\t}\n\n\t.inquiry-section {\n\t\tbackground: var(--card-background, #ffffff);\n\t\tborder-radius: var(--card-radius, 20px);\n\t\tpadding: 2rem;\n\t\tbox-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n\t}\n\n\t.inquiry-form {\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\tgap: 1.25rem;\n\t}\n\n\t.form-row {\n\t\tdisplay: grid;\n\t\tgrid-template-columns: repeat(auto-fit, minmax(200px, 1fr));\n\t\tgap: 1.25rem;\n\t}\n\n\t.form-group {\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\tgap: 0.5rem;\n\t}\n\n\t.form-group label {\n\t\tfont-size: 0.9rem;\n\t\tfont-weight: 600;\n\t\tcolor: var(--heading-color, #1a1a1a);\n\t}\n\n\t.form-group input,\n\t.form-group select,\n\t.form-group textarea {\n\t\tpadding: 0.875rem 1rem;\n\t\tborder: 2px solid var(--border-color, #e5e7eb);\n\t\tborder-radius: 10px;\n\t\tfont-size: 1rem;\n\t\tcolor: var(--body-text-color, #1a1a1a);\n\t\tbackground: var(--input-background, #ffffff);\n\t\tfont-family: inherit;\n\t\ttransition: border-color 0.2s;\n\t}\n\n\t.form-group input:focus,\n\t.form-group select:focus,\n\t.form-group textarea:focus {\n\t\toutline: none;\n\t\tborder-color: var(--accent-color, #a855f7);\n\t}\n\n\t.form-group textarea {\n\t\tresize: vertical;\n\t}\n\n\t.form-error {\n\t\tbackground: #fef2f2;\n\t\tcolor: #dc2626;\n\t\tpadding: 0.75rem 1rem;\n\t\tborder-radius: 8px;\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 0.5rem;\n\t\tfont-size: 0.9rem;\n\t}\n\n\t.submit-btn {\n\t\tdisplay: inline-flex;\n\t\talign-items: center;\n\t\tjustify-content: center;\n\t\tgap: 0.5rem;\n\t\tbackground: linear-gradient(90deg, var(--gradient-start, #a855f7), var(--gradient-end, #ec4899));\n\t\tcolor: #ffffff;\n\t\tpadding: 1rem 2rem;\n\t\tborder: none;\n\t\tborder-radius: 10px;\n\t\tfont-size: 1rem;\n\t\tfont-weight: 600;\n\t\t