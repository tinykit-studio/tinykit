	"frontend_code": "<script>\n\timport Icon from '@iconify/svelte'\n\timport content from '$content'\n\timport data from '$data'\n\n\tlet pages = $state([])\n\tlet loading = $state(true)\n\tlet active_page = $state(null)\n\tlet search = $state('')\n\tlet sidebar_open = $state(true)\n\n\tfunction get_page_from_url() {\n\t\tconst path = window.location.pathname.split('/').filter(Boolean)\n\t\treturn path[0] || null\n\t}\n\n\tfunction navigate_to(page_id) {\n\t\tconst url = new URL(window.location.href)\n\t\turl.pathname = '/' + page_id\n\t\twindow.history.pushState({}, '', url)\n\t}\n\n\tdata.pages.subscribe(items => {\n\t\tpages = items.sort((a, b) => (a.order || 0) - (b.order || 0))\n\t\tloading = false\n\t\tconst url_page = get_page_from_url()\n\t\tif (url_page && items.find(p => p.id === url_page)) {\n\t\t\tactive_page = url_page\n\t\t} else if (!active_page && items.length > 0) {\n\t\t\tactive_page = items[0].id\n\t\t}\n\t})\n\n\tlet current_page = $derived(pages.find(p => p.id === active_page))\n\n\tlet filtered_pages = $derived(\n\t\t!search.trim() \n\t\t\t? pages \n\t\t\t: pages.filter(p => \n\t\t\t\tp.title.toLowerCase().includes(search.toLowerCase()) ||\n\t\t\t\tp.content.toLowerCase().includes(search.toLowerCase())\n\t\t\t)\n\t)\n\n\tlet grouped_pages = $derived(() => {\n\t\tconst groups = {}\n\t\tfor (const page of filtered_pages) {\n\t\t\tconst section = page.section || 'General'\n\t\t\tif (!groups[section]) groups[section] = []\n\t\t\tgroups[section].push(page)\n\t\t}\n\t\treturn groups\n\t})\n\n\tfunction render_content(text) {\n\t\tif (!text) return ''\n\t\t// Handle code blocks first\n\t\ttext = text.replace(/```(\\w+)?\\n([\\s\\S]*?)```/g, (_, lang, code) => {\n\t\t\treturn `<pre><code class=\"language-${lang || 'text'}\">${escape_html(code.trim())}</code></pre>`\n\t\t})\n\t\treturn text\n\t\t\t.replace(/^### (.+)$/gm, '<h3>$1</h3>')\n\t\t\t.replace(/^## (.+)$/gm, '<h2>$1</h2>')\n\t\t\t.replace(/\\*\\*(.+?)\\*\\*/g, '<strong>$1</strong>')\n\t\t\t.replace(/\\*(.+?)\\*/g, '<em>$1</em>')\n\t\t\t.replace(/`(.+?)`/g, '<code>$1</code>')\n\t\t\t.replace(/\\n\\n/g, '</p><p>')\n\t\t\t.replace(/\\n/g, '<br>')\n\t\t\t.replace(/^(?!<)/, '<p>')\n\t\t\t.replace(/(?!>)$/, '</p>')\n\t}\n\n\tfunction escape_html(text) {\n\t\treturn text\n\t\t\t.replace(/&/g, '&amp;')\n\t\t\t.replace(/</g, '&lt;')\n\t\t\t.replace(/>/g, '&gt;')\n\t}\n\n\tfunction get_method_color(method) {\n\t\tconst colors = {\n\t\t\t'GET': '#22c55e',\n\t\t\t'POST': '#3b82f6',\n\t\t\t'PUT': '#f59e0b',\n\t\t\t'PATCH': '#8b5cf6',\n\t\t\t'DELETE': '#ef4444'\n\t\t}\n\t\treturn colors[method] || '#64748b'\n\t}\n</script>\n\n<div class=\"page\" class:sidebar-collapsed={!sidebar_open}>\n\t<aside class=\"sidebar\">\n\t\t<div class=\"sidebar-header\">\n\t\t\t<div class=\"logo\">\n\t\t\t\t<Icon icon=\"lucide:code-2\" width={20} />\n\t\t\t\t<span>{content.title}</span>\n\t\t\t</div>\n\t\t\t{#if content.version}\n\t\t\t\t<span class=\"version\">{content.version}</span>\n\t\t\t{/if}\n\t\t</div>\n\n\t\t{#if sidebar_open}\n\t\t\t<div class=\"search-box\">\n\t\t\t\t<Icon icon=\"lucide:search\" width={16} class=\"search-icon\" />\n\t\t\t\t<input type=\"text\" placeholder=\"Search...\" bind:value={search} />\n\t\t\t</div>\n\n\t\t\t<nav class=\"nav\">\n\t\t\t\t{#each Object.entries(grouped_pages()) as [section, section_pages] (section)}\n\t\t\t\t\t<div class=\"nav-section\">\n\t\t\t\t\t\t<span class=\"section-title\">{section}</span>\n\t\t\t\t\t\t{#each section_pages as page (page.id)}\n\t\t\t\t\t\t\t<button \n\t\t\t\t\t\t\t\tclass=\"nav-item\"\n\t\t\t\t\t\t\t\tclass:active={active_page === page.id}\n\t\t\t\t\t\t\t\tonclick={() => { active_page = page.id; search = ''; navigate_to(page.id) }}\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t{#if page.method}\n\t\t\t\t\t\t\t\t\t<span class=\"method\" style=\"--method-color: {get_method_color(page.method)}\">\n\t\t\t\t\t\t\t\t\t\t{page.method}\n\t\t\t\t\t\t\t\t\t</span>\n\t\t\t\t\t\t\t\t{/if}\n\t\t\t\t\t\t\t\t<span class=\"nav-title\">{page.title}</span>\n\t\t\t\t\t\t\t</button>\n\t\t\t\t\t\t{/each}\n\t\t\t\t\t</div>\n\t\t\t\t{/each}\n\t\t\t</nav>\n\t\t{/if}\n\t</aside>\n\n\t<main class=\"content\">\n\t\t<button class=\"toggle-btn\" onclick={() => sidebar_open = !sidebar_open}>\n\t\t\t<Icon icon={sidebar_open ? 'lucide:panel-left-close' : 'lucide:panel-left-open'} width={18} />\n\t\t</button>\n\n\t\t{#if loading}\n\t\t\t<div class=\"loading\">\n\t\t\t\t<Icon icon=\"lucide:loader-2\" width={24} class=\"spin\" />\n\t\t\t</div>\n\t\t{:else if current_page}\n\t\t\t<article class=\"article\">\n\t\t\t\t{#if current_page.method}\n\t\t\t\t\t<div class=\"endpoint-header\">\n\t\t\t\t\t\t<span class=\"method-badge\" style=\"--method-color: {get_method_color(current_page.method)}\">\n\t\t\t\t\t\t\t{current_page.method}\n\t\t\t\t\t\t</span>\n\t\t\t\t\t\t<code class=\"endpoint-path\">{current_page.endpoint || '/'}</code>\n\t\t\t\t\t</div>\n\t\t\t\t{/if}\n\t\t\t\t<h1>{current_page.title}</h1>\n\t\t\t\t{#if current_page.description}\n\t\t\t\t\t<p class=\"description\">{current_page.description}</p>\n\t\t\t\t{/if}\n\t\t\t\t<div class=\"prose\">\n\t\t\t\t\t{@html render_content(current_page.content)}\n\t\t\t\t</div>\n\t\t\t</article>\n\t\t{:else}\n\t\t\t<div class=\"empty\">\n\t\t\t\t<Icon icon=\"lucide:file-code\" width={48} />\n\t\t\t\t<p>{content.empty_text}</p>\n\t\t\t</div>\n\t\t{/if}\n\t</main>\n</div>\n\n<style>\n\t.page {\n\t\tmin-height: 100vh;\n\t\tbackground: var(--bg-color, #0f172a);\n\t\tdisplay: grid;\n\t\tgrid-template-columns: 280px 1fr;\n\t\tfont-family: var(--font, system-ui, sans-serif);\n\t}\n\n\t.page.sidebar-collapsed {\n\t\tgrid-template-columns: 0 1fr;\n\t}\n\n\t.sidebar {\n\t\tbackground: var(--sidebar-bg, #1e293b);\n\t\tborder-right: 1px solid var(--border-color, #334155);\n\t\toverflow: hidden;\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t}\n\n\t.sidebar-collapsed .sidebar {\n\t\tborder: none;\n\t}\n\n\t.sidebar-header {\n\t\tpadding: 1rem;\n\t\tborder-bottom: 1px solid var(--border-color, #334155);\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tjustify-content: space-between;\n\t}\n\n\t.logo {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 0.5rem;\n\t\tcolor: var(--heading-color, #f8fafc);\n\t\tfont-weight: 600;\n\t\tfont-size: 0.9375rem;\n\t}\n\n\t.version {\n\t\tpadding: 0.25rem 0.5rem;\n\t\tbackground: var(--accent-color, #3b82f6);\n\t\tcolor: white;\n\t\tborder-radius: 4px;\n\t\tfont-size: 0.75rem;\n\t\tfont-weight: 600;\n\t}\n\n\t.search-box {\n\t\tpadding: 0.75rem 1rem;\n\t\tposition: relative;\n\t}\n\n\t.search-box :global(.search-icon) {\n\t\tposition: absolute;\n\t\tpadding: 0.5rem;\n\t\ttop: 50%;\n\t\tleft: 1rem;\n\t\ttransform: translateY(-50%);\n\t\tcolor: var(--text-color, #94a3b8);\n\t}\n\n\t.search-box input {\n\t\twidth: 100%;\n\t\tpadding: 0.5rem 0.75rem 0.5rem 2.25rem;\n\t\tbackground: var(--input-bg, #0f172a);\n\t\tborder: 1px solid var(--border-color, #334155);\n\t\tborder-radius: 6px;\n\t\tcolor: var(--heading-color, #f8fafc);\n\t\tfont-size: 0.875rem;\n\t}\n\n\t.search-box input::placeholder {\n\t\tcolor: var(--text-color, #64748b);\n\t}\n\n\t.search-box input:focus {\n\t\toutline: none;\n\t\tborder-color: var(--accent-color, #3b82f6);\n\t}\n\n\t.nav {\n\t\tflex: 1;\n\t\toverflow-y: auto;\n\t\tpadding: 0.5rem 0;\n\t}\n\n\t.nav-section {\n\t\tmargin-bottom: 1rem;\n\t}\n\n\t.section-title {\n\t\tdisplay: block;\n\t\tpadding: 0.5rem 1rem;\n\t\tfont-size: 0.6875rem;\n\t\tfont-weight: 600;\n\t\ttext-transform: uppercase;\n\t\tletter-spacing: 0.05em;\n\t\tcolor: var(--text-color, #64748b);\n\t}\n\n\t.nav-item {\n\t\twidth: 100%;\n\t\tpadding: 0.5rem 1rem;\n\t\tbackground: none;\n\t\tborder: none;\n\t\ttext-align: left;\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 0.5rem;\n\t\tcolor: var(--text-color, #94a3b8);\n\t\tfont-size: 0.875rem;\n\t\tcursor: pointer;\n\t\ttransition: all 0.15s;\n\t}\n\n\t.nav-item:hover {\n\t\tbackground: var(--hover-bg, rgba(255,255,255,0.05));\n\t\tcolor: var(--heading-color, #f8fafc);\n\t}\n\n\t.nav-item.active {\n\t\tbackground: color-mix(in srgb, var(--accent-color, #3b82f6) 15%, transparent);\n\t\tcolor: var(--accent-color, #3b82f6);\n\t}\n\n\t.method {\n\t\tfont-size: 0.625rem;\n\t\tfont-weight: 700;\n\t\tpadding: 0.125rem 0.375rem;\n\t\tbackground: color-mix(in srgb, var(--method-color) 20%, transparent);\n\t\tcolor: var(--method-color);\n\t\tborder-radius: 3px;\n\t\tflex-shrink: 0;\n\t}\n\n\t.nav-title {\n\t\toverflow: hidden;\n\t\ttext-overflow: ellipsis;\n\t\twhite-space: nowrap;\n\t}\n\n\t.content {\n\t\toverflow-y: auto;\n\t\tpadding: 2rem 3rem;\n\t\tmax-height: 100vh;\n\t\tposition: relative;\n\t}\n\n\t.toggle-btn {\n\t\tposition: absolute;\n\t\ttop: 1rem;\n\t\tleft: 1rem;\n\t\tpadding: 0.375rem;\n\t\tbackground: var(--sidebar-bg, #1e293b);\n\t\tborder: 1px solid var(--border-color, #334155);\n\t\tcolor: var(--text-color, #94a3b8);\n\t\tcursor: pointer;\n\t\tborder-radius: 6px;\n\t}\n\n\t.toggle-btn:hover {\n\t\tcolor: var(--heading-color, #f8fafc);\n\t}\n\n\t.loading, .empty {\n\t\tmax-width: 720px;\n\t\tmargin: 4rem auto;\n\t\ttext-align: center;\n\t\tcolor: var(--text-color, #64748b);\n\t}\n\n\t.empty p { margin-top: 1rem; }\n\n\t.article {\n\t\tmax-width: 720px;\n\t\tmargin: 0 auto;\n\t}\n\n\t.endpoint-header {\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tgap: 0.75rem;\n\t\tmargin-bottom: 1rem;\n\t\tpadding: 0.75rem 1rem;\n\t\tbackground: var(--sidebar-bg, #1e293b);\n\t\tborder-radius: 8px;\n\t}\n\n\t.method-badge {\n\t\tfont-size: 0.75rem;\n\t\tfont-weight: 700;\n\t\tpadding: 0.25rem 0.5rem;\n\t\tbackground: var(--method-color);\n\t\tcolor: white;\n\t\tborder-radius: 4px;\n\t}\n\n\t.endpoint-path {\n\t\tfont-family: ui-monospace, monospace;\n\t\tfont-size: 0.9375rem;\n\t\tcolor: var(--heading-color, #f8fafc);\n\t}\n\n\t.article h1 {\n\t\tfont-size: 2rem;\n\t\tfont-weight: 700;\n\t\tcolor: var(--heading-color, #f8fafc);\n\t\tmargin: 0 0 0.75rem;\n\t}\n\n\t.description {\n\t\tfont-size: 1.0625rem;\n\t\tcolor: var(--text-color, #94a3b8);\n\t\tmargin: 0 0 2rem;\n\t\tline-height: 1.6;\n\t}\n\n\t.prose {\n\t\tcolor: var(--prose-color, #cbd5e1);\n\t\tline-height: 1.8;\n\t\tfont-size: 0.9375rem;\n\t}\n\n\t.prose :global(h2) {\n\t\tfont-size: 1.375rem;\n\t\tfont-weight: 600;\n\t\tcolor: var(--heading-color, #f8fafc);\n\t\tmargin: 2.5rem 0 1rem;\n\t}\n\n\t.prose :global(h3) {\n\t\tfont-size: 1.125rem;\n\t\tfont-weight: 600;\n\t\tcolor: var(--heading-color, #f8fafc);\n\t\tmargin: 2rem 0 0.75rem;\n\t}\n\n\t.prose :global(p) {\n\t\tmargin: 0 0 1.25rem;\n\t}\n\n\t.prose :global(strong) {\n\t\tcolor: var(--heading-color, #f8fafc);\n\t\tfont-weight: 600;\n\t}\n\n\t.prose :global(code) {\n\t\tbackground: var(--code-bg, #1e293b);\n\t\tpadding: 0.125rem 0.375rem;\n\t\tborder-radius: 4px;\n\t\tfont-size: 0.875em;\n\t\tfont-family: ui-monospace, monospace;\n\t\tcolor: var(--code-color, #e879f9);\n\t}\n\n\t.prose :global(pre) {\n\t\tbackground: var(--code-bg, #1e293b);\n\t\tpadding: 1rem 1.25rem;\n\t\tborder-radius: 8px;\n\t\toverflow-x: auto;\n\t\tmargin: 1.5rem 0;\n\t}\n\n\t.prose :global(pre code) {\n\t\tbackground: none;\n\t\tpadding: 0;\n\t\tcolor: var(--prose-color, #cbd5e1);\n\t\tfont-size: 0.875rem;\n\t\tline-height: 1.7;\n\t}\n\n\t:global(.spin) {\n\t\tanimation: spin 1s linear infinite;\n\t}\n\n\t@keyframes spin {\n\t\tfrom { transform: rotate(0deg); }\n\t\tto { transform: rotate(360deg); }\n\t}\n</style>",
