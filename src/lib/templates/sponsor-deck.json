{
	"id": "sponsor-deck",
	"name": "Sponsor Deck",
	"description": "Partnership pitch deck with packages and testimonials",
	"preview": "lucide:presentation",
	"tags": [
		"creator",
		"intermediate",
		"business"
	],
	"kits": [
		"creator"
